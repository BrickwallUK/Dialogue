@using Dialogue.Logic.Application.ExtensionMethods
@inherits UmbracoViewPage<Dialogue.Logic.Models.LogOnModel>
@{
    Layout = "OneColThin.cshtml";
}

@section PageTitle {
    @Model.PageTitle
}

@section MetaDescription
{
    @Model.MetaDesc
}

@if (!Model.ShowForgotPassword)
{
    @Html.Action("LogOnForm", "DialogueLogin")
}
else
{
    @Html.Action("ForgotPasswordForm", "DialogueLogin")
}

